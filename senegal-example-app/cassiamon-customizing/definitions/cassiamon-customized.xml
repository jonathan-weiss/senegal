<?xml version="1.0" encoding="utf-8" ?>
<cassiamon xmlns="https://cassiamon.ch/cassiamon-schemagic"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://cassiamon.ch/cassiamon-schemagic ./schema/cassiamon-schemagic-schema.xsd">
    <definitions>
        <entity entityName="Person" conceptIdentifier="Person">
            <entitySimpleAttribute entityAttributeName="firstname" entityAttributeType="Text"/>
            <entitySimpleAttribute entityAttributeName="lastname" entityAttributeType="Text"/>
            <entitySimpleAttribute entityAttributeName="nickname" entityAttributeType="Text"/>
            <entityReferenceAttribute entityReferenceName="address" referencedEntity="Address"/>
        </entity>
        <entity entityName="Address" conceptIdentifier="Address">
            <entitySimpleAttribute entityAttributeName="street" entityAttributeType="Text"/>
            <entitySimpleAttribute entityAttributeName="zip" entityAttributeType="IntegerNumber"/>
        </entity>
        <entity entityName="Department">
            <entitySimpleAttribute entityAttributeName="title" entityAttributeType="Text"/>
        </entity>
        <entity entityName="DepartmentAddress">
            <entitySimpleAttribute entityAttributeName="street" entityAttributeType="Text"/>
            <entitySimpleAttribute entityAttributeName="phone" entityAttributeType="IntegerNumber"/>
        </entity>
        <entity entityName="OfficeBrackets">
            <entitySimpleAttribute entityAttributeName="stupidLevel" entityAttributeType="Text"/>
        </entity>
    </definitions>

</cassiamon>
