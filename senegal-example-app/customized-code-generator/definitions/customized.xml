<?xml version="1.0" encoding="utf-8" ?>
<senegal xmlns="https://senegal.ch/senegal"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://senegal.ch/senegal schema/senegal-schema.xsd"
>

    <entity kotlinModelClassName="Person" kotlinModelPackage="ch.senegal.person">
        <entityAttribute kotlinFieldName="firstname" kotlinFieldType="kotlin.String"/>
        <entityAttribute kotlinFieldName="lastname" kotlinFieldType="kotlin.String"/>
        <entityAttribute kotlinFieldName="nickname" kotlinFieldType="kotlin.String"/>
    </entity>

    <entity kotlinModelClassName="Address" kotlinModelPackage="ch.senegal.address">
        <entityAttribute kotlinFieldName="street" kotlinFieldType="kotlin.String"/>
        <entityAttribute kotlinFieldName="zip" kotlinFieldType="kotlin.Int"/>
    </entity>

</senegal>
