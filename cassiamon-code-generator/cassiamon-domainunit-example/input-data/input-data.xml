<?xml version="1.0" encoding="utf-8" ?>
<cassiamon xmlns="https://cassiamon.ch/cassiamon-schemagic"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://cassiamon.ch/cassiamon-schemagic ./schema/cassiamon-schemagic-schema.xsd">
    <configuration/>
    <definitions>
        <entity name="Person" alternativeName="PERSON">
            <entityAttribute attributeName="firstname"/>
            <entityAttribute attributeName="lastname"/>
            <entityAttribute attributeName="nickname"/>
        </entity>
        <entity name="Address">
            <entityAttribute attributeName="street"/>
            <entityAttribute attributeName="zip"/>
        </entity>
    </definitions>
</cassiamon>
